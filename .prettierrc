{
  "semi": true,
  "trailingComma": "es5",
  "singleQuote": true,
  "printWidth": 150,
  "tabWidth": 2,
  "useTabs": false,
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": ["<BUILTIN_MODULES>", "^react", "^next", "<THIRD_PARTY_MODULES>", "^@", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
